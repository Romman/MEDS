            <key-property name="shopId" type="int"><column name="shop_id" /></key-property>
            <key-property name="itemTemplateId" type="int"><column name="item_template_id" /></key-property>
        </composite-id>
        <property name="count" type="int">
            <column name="count" />
        </property>
    </class>

    <class name="Skill" table="`skill`">
        <id name="id" type="int" column="id">
            <generator class="increment" />
        </id>
        <property name="name" type="string">
            <column name="name" />
        </property>
    </class>

    <class name="Spell" table="`spell`">
        <id name="id" type="int" column="id">
            <generator class="increment" />
        </id>
        <property name="typeInteger" type="int">
            <column name="type" />
        </property>
        <property name="name" type="string">
            <column name="name" />
        </property>
    </class>

</hibernate-mapping>
